{
  "name": "ponticlaro/bebop-media",
  "description": "WordPress Plugin to manage media in the cloud",
  "keywords": ["wordpress", "storage", "cloud"],
  "type": "wordpress-plugin",
  "license": [
    "MIT",
    "GPL-3.0+"
  ],
  "authors": [
    {
      "name": "Ponticlaro",
      "email": "hello@ponticlaro.com",
      "homepage": "http://www.ponticlaro.com",
      "role": "Developer"
    }
  ],
  "require": {
    "ext-gd": "*",
    "composer/installers": "~1.0",
    "ponticlaro/bebop-cms": "^1",
    "ponticlaro/bebop-mvc": "^1",
    "ponticlaro/bebop-ui": "^1",
    "ponticlaro/bebop-http-api": "^1",
    "ponticlaro/bebop-scripts-loader": "^1",
    "respect/validation": "~0.9",
    "slim/slim": "^4",
    "league/flysystem": "^1",
    "league/flysystem-aws-s3-v3": "^1",
    "superbalist/flysystem-google-storage": "^7",
    "symfony/dom-crawler": "^2.7",
    "symfony/css-selector": "^2.7"
  },
  "require-dev": {
    "apigen/apigen": "^4"
  },
  "autoload": {
    "psr-4": {
      "Ponticlaro\\Bebop\\Media\\": "lib/"
    }
  },
  "scripts": {
    "generate-docs": "apigen generate --config ./.apigen.yaml"
  },
  "support": {
    "email": "hello@ponticlaro.com",
    "issues": "https://github.com/ponticlaro/bebop-media/issues",
    "forum": "https://gitter.im/ponticlaro/bebop"
  }
}
