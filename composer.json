{
  "name": "ponticlaro/bebop-media",
  "type": "wordpress-plugin",
  "description": "WordPress Plugin to manage media in the cloud",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:ponticlaro/ponticlaro-aws.git"
    }
  ],
  "require": {
    "ext-gd": "*",
    "composer/installers": "~1.0",
    "ponticlaro/bebop-cms": "^1",
    "ponticlaro/bebop-mvc": "^1",
    "ponticlaro/bebop-ui": "^1",
    "ponticlaro/bebop-http-api": "^1",
    "ponticlaro/bebop-scripts-loader": "^1",
    "respect/validation": "~0.9",
    "slim/slim": "~2",
    "league/flysystem": "~1",
    "league/flysystem-aws-s3-v3": "^1",
    "superbalist/flysystem-google-storage": "^3",
    "symfony/dom-crawler": "^2.7",
    "symfony/css-selector": "^2.7"
  },
  "require-dev": {
    "apigen/apigen": "^4"
  },
  "autoload": {
    "psr-4": {
      "Ponticlaro\\Bebop\\Media\\": "lib/"
    }
  },
  "scripts": {
    "generate-docs": "apigen generate --config ./apigen.yaml"
  },
  "suggest": {
    "ponticlaro/aws-transcoder": "Allows usage of AWS Elastic Transcoder to optimize video uploads"
  }
}
